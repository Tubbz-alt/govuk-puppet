<LoadPlugin python>
  Globals true
</LoadPlugin>

<Plugin python>
  Import "collectd_cdn.fastly"

  <Module "collectd_cdn.fastly">
    ApiKey "<%= api_key -%>"

<% @services.each_pair do |name, id| -%>
    <Service>
      Name "<%= name -%>"
      Id "<%= id -%>"
    </Service>
<% end -%>
  </Module>
</Plugin>
